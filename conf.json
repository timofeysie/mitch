{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["app/app.js",
            "app/view1",
            "app/view2"],
        "exclude": [ /* array of paths to exclude */ ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}